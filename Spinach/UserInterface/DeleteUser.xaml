<Window x:Class="Spinach.DeleteUser"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="DeleteUser" Icon="/Spinach;component/Icon.JPG" WindowStartupLocation="CenterScreen" Height="250" Width="250">
    <Grid Background="Lavender">
        <ComboBox Height="23" Margin="49,61,0,0" Name="cbUser" VerticalAlignment="Top" HorizontalAlignment="Left" Width="120">
            <ComboBoxItem>Arun</ComboBoxItem>
            <ComboBoxItem>Abhay</ComboBoxItem>
            <ComboBoxItem>Prateek</ComboBoxItem>
            <ComboBoxItem>Rutu</ComboBoxItem>
        </ComboBox>
        <Button Height="23" Margin="49,0,0,61" Name="btnDelete" VerticalAlignment="Bottom" Click="btnDelete_Click" HorizontalAlignment="Left" Width="50">Delete</Button>
        <Button Height="23" Margin="119,0,0,61" Name="btnCancel" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="50" Click="btnCancel_Click">Cancel</Button>
    </Grid>
</Window>
